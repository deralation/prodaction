<%= form_for([equip, Availability.new]) do |f|%>

<div class="row">
  <div class="col-md-6">
  <label>Start Date</label>
    <%= f.date_field :start_date, placeholder: 'Start Date' %>
  </div>
  <div class="col-md-6">
  <label>End Date</label>
    <%= f.date_field :end_date, placeholder: 'End Date' %>
  </div>
</div>
  <%= f.hidden_field :equipment_id, value: equip.id %>
  <br>
  <%= f.submit "Add availability"%>
<% end %>

