<div class="global-wrap">
        <div class="container">
            <div class="booking-item-details">
                <header class="booking-item-header">
                    <div class="row">
                        <div class="col-md-8">
                            <h2 class="lh1em"><%= @equipment.name %></h2>
                            <p class="lh1em text-small"><i class="fa fa-map-marker"></i> <%= @equipment.address %></p>
                        </div>
                        <div class="col-md-4 price">
                            <h2><div class="show-price pull-right">â‚¬ <%= @equipment.value %> </div></h2>
                            <h4><div class="perday">/ day</div></h4>
                        </div>
                    </div>
                </header>
                <div class="row">
                    <div class="col-md-8">
                        <div class="tabbable booking-details-tabbable">
                            <ul class="nav nav-tabs" id="myTab">
                                <li class="active"><a href="#tab-1" data-toggle="tab"><i class="fa fa-camera"></i>Photos</a>
                                </li>
                                <li><a href="#google-map-tab" data-toggle="tab"><i class="fa fa-map-marker"></i>On the Map</a>
                                </li>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane fade in active" id="tab-1">
                                    <div class="fotoshow" >
                                       <%= image_tag @equipment.picture.url(:big) %>


                                    </div>
                                </div>
                                <div class="tab-pane fade" id="google-map-tab">
                                    <div id="map-canvas" style="width:100%; height:500px;"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="booking-item-dates-change">
                            <form>
                                <div class="input-daterange" data-date-format="MM d, DD">
                                    <div class="row">
                                      <%= render "reservations/form" %>

                                    </div>
                                </div>

                            </form>
                        </div>
                        <!-- START PROFILE -->
                        <div class="row">
                        <div class= "col-md-6 profile">
