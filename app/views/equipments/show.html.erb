                          <h4><%= @equipment.user.first_name %> <%= @equipment.user.last_name %></h4>

                          <a href="#" class="btn btn-primary btn-lg" id= "Meet">Contact</a>



                       </div>
                       </div>


                    </div>



                    </div>
                </div>
                <div class="gap"></div>
                <div class="row">

                    <div class="col-md-6">
